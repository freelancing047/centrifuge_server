            <condition />
            <validation>import csi.installer.InstallationHelper;
import csi.license.*;

FormComponent comp = formEnvironment.getFormComponentById("26");
JTextField licenseInput = (JTextField) comp.getConfigurationObject();

String value = licenseInput.getText();
value = ( value == null ) ? "" : value.trim();

boolean proceed = (value.length() &gt; 0 );
if( proceed ) {
    InstallerLicense license = InstallationHelper.createLicense( licenseInput );
    proceed = (license != null );
    if( !proceed ) {
        Util.showErrorMessage(context.getMessage("LicenseKeyInvalid"));
    } else if( InstallationHelper.isExpired( license ) ) {
        Util.showErrorMessage(context.getMessage("LicenseKeyExpired"));
    }
}


return true;</validation>
