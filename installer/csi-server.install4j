            <postActivation>final int MEMORY_DIVIDER = 1024 * 1024 * 1024;

long systemMemory = SystemInfo.getPhysicalMemory();

int gigsMemory = (int)(systemMemory / MEMORY_DIVIDER);
int suggestedMemory = (int)(systemMemory / 2 / MEMORY_DIVIDER);

if(gigsMemory == 0){
context.setVariable("JVM_OPTIONS_VARIABLE", new String[]{"512m"});
    return;
}

List&lt;String&gt; memList = new ArrayList&lt;String&gt;();
int memory = 1;
int index = 0;
for(int ii=1; memory &lt;= gigsMemory; ii++){
    memList.add(memory + "g");
    if(memory &lt; suggestedMemory){
        context.setVariable("JVM_SUGGESTED_INDEX", index);
    }
    index++;
    memory = (int)Math.pow(2, ii);

}

context.setVariable("JVM_OPTIONS_VARIABLE", memList.toArray());
return;</postActivation>
