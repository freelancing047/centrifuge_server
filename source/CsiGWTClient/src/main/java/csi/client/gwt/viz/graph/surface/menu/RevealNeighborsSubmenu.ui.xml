<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
    xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:boot="urn:import:com.github.gwtbootstrap.client.ui.base">
	<ui:style>
		.block {
			display: block;
		}
	</ui:style>
    <g:HTMLPanel>
	    <boot:UnorderedList addStyleNames="{style.block}" ui:field="navList" />
	</g:HTMLPanel>
</ui:UiBinder> 