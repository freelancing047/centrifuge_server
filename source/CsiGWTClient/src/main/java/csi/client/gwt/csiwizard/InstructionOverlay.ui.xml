<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:gwt="urn:import:com.google.gwt.user.client.ui"
	xmlns:boot="urn:import:com.github.gwtbootstrap.client.ui"
    xmlns:csigwt="urn:import:csi.client.gwt.widget.ui"
    xmlns:csiboot="urn:import:csi.client.gwt.widget.boot">	
	<ui:style>
		.nocolor{
			background-color: transparent
		}
		.noborder{
		    border-style: none;
		}
	</ui:style>
 
    <csiboot:CsiModal ui:field="dialog" height="454px" width="820px" addStyleNames="{style.noborder}">
		<csigwt:FullSizeLayoutPanel ui:field="clearPanel">
            <csigwt:layer top="70px" bottom="70px" left="512px" right="20px">
				<csigwt:FullSizeLayoutPanel ui:field="whitePanel">
				</csigwt:FullSizeLayoutPanel>
             </csigwt:layer>
        </csigwt:FullSizeLayoutPanel>
    </csiboot:CsiModal>
</ui:UiBinder>
