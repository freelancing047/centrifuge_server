<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			xmlns:csiboot="urn:import:csi.client.gwt.widget.boot"
			xmlns:csibutton="urn:import:csi.client.gwt.widget.buttons"
			xmlns:csigwt="urn:import:csi.client.gwt.widget.ui"
			xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
		.bigLabel {
			font-size: 24pt;
			line-height: normal;
		}
	</ui:style>

    <ui:with field="i18n" type="csi.client.gwt.i18n.CentrifugeConstantsLocator"/>

	<csigwt:FullSizeLayoutPanel>
		<csigwt:layer right="20" width="90" top="5" height="30">
			<csibutton:SimpleButton text="{i18n.get.dialog_ExitButton}" ui:field="exitButton"/>
		</csigwt:layer>
		<csigwt:layer left="20" right="20" top="40" bottom="0">
			<csiboot:CsiTabPanel tabPosition="top" width="100%" height="100%" ui:field="tabPanel">
			</csiboot:CsiTabPanel>
		</csigwt:layer>
	</csigwt:FullSizeLayoutPanel>

</ui:UiBinder>