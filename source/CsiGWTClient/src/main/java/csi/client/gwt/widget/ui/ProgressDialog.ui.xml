<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:boot="urn:import:com.github.gwtbootstrap.client.ui"
             xmlns:csiboot="urn:import:csi.client.gwt.widget.boot">

   <ui:style gss="true">
		.container {
			padding: 10px;
		}
    </ui:style>

    <ui:with field="resources" type="csi.client.gwt.resources.ApplicationResources" />

    <csiboot:Dialog bodyWidth="550px" bodyHeight="150px">
    	<g:FlowPanel addStyleNames="{style.container}">
    		<g:Label ui:field="counterLabel"/>
    		<boot:ProgressBar type="ANIMATED" ui:field="counterBar"/>
    		<g:Label ui:field="progressLabel"/>
    		<boot:ProgressBar type="ANIMATED" ui:field="progressBar"/>
    	</g:FlowPanel>
    </csiboot:Dialog>


</ui:UiBinder>