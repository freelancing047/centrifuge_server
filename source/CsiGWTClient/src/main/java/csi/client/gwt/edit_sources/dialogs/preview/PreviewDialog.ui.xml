<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container"
	xmlns:gxtbutton="urn:import:com.sencha.gxt.widget.core.client.button"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client"
	xmlns:boot="urn:import:com.github.gwtbootstrap.client.ui"
	xmlns:gxtform="urn:import:com.sencha.gxt.widget.core.client.form"
	xmlns:gxtgrid="urn:import:com.sencha.gxt.widget.core.client.grid"
	xmlns:csiboot="urn:import:csi.client.gwt.widget.boot" xmlns:grid="urn:import:csi.client.gwt.widget.gxt.grid"
	xmlns:gwt="urn:import:csi.client.gwt.widget.ui" xmlns:this="urn:import:csi.client.gwt.viz.shared.filter" xmlns:base="urn:import:com.github.gwtbootstrap.client.ui.base">

	<ui:style>
		.compactLabel {
			width: 100px !important;
		}
		
		.compactControls {
			margin-left: 120px !important;
		}
	</ui:style>

	<ui:with field="resources" type="csi.client.gwt.resources.ApplicationResources" />

    <ui:with field="title" type="java.lang.String" />
	
	<csiboot:Dialog title="{title}" bodyWidth="600px" bodyHeight="100px">
		<gwt:FullSizeLayoutPanel ui:field="mainContainer">
			<gwt:layer>
				<base:DivWidget ui:field="gridWrapper"/>
			</gwt:layer>
		</gwt:FullSizeLayoutPanel>
	</csiboot:Dialog>

</ui:UiBinder>